{
  "expo": {
    "name": "denticheck-app",
    "slug": "denticheck-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/shared/assets/icon.png",
    "scheme": "denticheck",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/shared/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.denticheck.app"
    },
    "android": {
      "package": "com.denticheck.app.temp",
      "adaptiveIcon": {
        "foregroundImage": "./src/shared/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/shared/assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-secure-store",
      "expo-web-browser",
      "@react-native-google-signin/google-signin"
    ]
  }
}
